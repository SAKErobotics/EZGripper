<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find ezgripper_dual_gen2_triple_mount_moveit_config)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="--namespace=/ezgripper_dual_gen2_triple_mount joint_state_controller ezgripper_controller --timeout 20"/>

</launch>
