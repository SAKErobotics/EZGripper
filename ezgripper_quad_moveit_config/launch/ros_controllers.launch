<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find ezgripper_quad_moveit_config)/config/ros_controllers.yaml" command="load"/>

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" ns="/ezgripper_quad" output="screen"
		args="--namespace=/ezgripper_quad
		ezgripper_controller
		--timeout 20"/>

</launch>
